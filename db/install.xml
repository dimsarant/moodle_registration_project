<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/project/db" VERSION="20220826" COMMENT="XMLDB file for local_project"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="registration_data" COMMENT="Config table used for registration">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" SEQUENCE="true"/>
                <FIELD NAME="firstname" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="lastname" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="email" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="country" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="mobile" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="password" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
                <FIELD NAME="status" TYPE="int" LENGTH="1" NOTNULL="true" SEQUENCE="false" DEFAULT="-1"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
